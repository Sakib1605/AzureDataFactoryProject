{
	"name": "SQLToAzure",
	"properties": {
		"activities": [
			{
				"name": "Copy SQL DATA",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "Latest Load Time",
						"dependencyConditions": [
							"Succeeded"
						]
					},
					{
						"activity": "LastLoadLookUp",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
